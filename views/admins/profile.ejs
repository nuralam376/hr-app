<% include ../layouts/admin/header %>
    <h1>Admin ID : <%= adminInfo.seq_id %>
        <div class="pull-right">
            <a href="/admin/profile/edit" class = "btn btn-info">Edit</a>
        </div>
    </h1>
    <table class = "table table-bordered">
            <tr>
                <th>Name</th>
                <td><%= adminInfo.name %></td>
            </tr>
            <tr>
                <th>Email</th>
                <td><%= adminInfo.email %></td>
            </tr>
            <tr>
                <th>Contact</th>
                <td><%= adminInfo.contact %></td>
            </tr>
            <tr>
                <th>Address</th>
                <td><%= adminInfo.address %></td>
            </tr>
            <tr>
                <th>Profile Photo</th>
                <% if (admin.profile_photo == "dummy.jpeg") { %>
                    <td><a data-fancybox="gallery" href="https://s3.ap-south-1.amazonaws.com/hr-app-test/<%= adminInfo.profile_photo %>"><img src = "https://s3.ap-south-1.amazonaws.com/hr-app-test/<%= adminInfo.profile_photo %>" height = "50px" width = "50px"></a></td>
                <% } else { %>
                    <td><a data-fancybox="gallery" href="https://s3.ap-south-1.amazonaws.com/hr-app-test/<%= adminInfo.company %>/admins/<%= adminInfo.profile_photo %>"><img src = "https://s3.ap-south-1.amazonaws.com/hr-app-test/<%= adminInfo.company %>/admins/<%= adminInfo.profile_photo %>" height = "300px" width = "500px"></a></td>
                <% } %>
            </tr>
        </table>

<% include ../layouts/admin/footer %>