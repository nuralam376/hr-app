<% include ../layouts/admin/header %>
<div class="container mt-xs-5 mb-xs-5">
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
            <h4>PAX TimeLine</h4>
            <%if (events.length > 0){ %>
                
                <ul class="timeline">
                <% events.forEach(user => { %>
                    <li>
                        <a href="javascript:void(0)"><%= user.display_name %></a>
                        <a href="javascript:void(0)" class="pull-right"><%= moment(user.time).format('llll') %></a>
                        <p><%= user.description %></p>
                    </li>
                <% }); %>    
                </ul>
            <% } else { %>
                <h1>No Events</h1>
            <% } %>
		</div>
	</div>
</div>

<% include ../layouts/admin/footer %>